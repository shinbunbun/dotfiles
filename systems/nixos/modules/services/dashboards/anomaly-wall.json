          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "${datasource}"
          },
          "format": 1,
          "rawSql": "SELECT \n  service,\n  count() as count\nFROM logs.anomalies\nWHERE detected_at >= now() - INTERVAL 24 HOUR\nGROUP BY service\nORDER BY count DESC\nLIMIT 10",
          "refId": "A"
        }
      ],
      "title": "サービス別異常分布（24時間）",
      "type": "piechart"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "${datasource}"
