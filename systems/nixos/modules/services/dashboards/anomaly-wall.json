        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "detected_at"
          }
        ]
      },
      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "${datasource}"
          },
          "format": 1,
          "rawSql": "SELECT \n  detected_at,\n  host,\n  service,\n  anomaly_type,\n  score,\n  details\nFROM logs.anomalies\nWHERE detected_at >= now() - INTERVAL 1 HOUR\nORDER BY score ASC\nLIMIT 50",
          "refId": "A"
        }
      ],
      "title": "異常ウォール - リアルタイム検知（過去1時間）",
      "type": "table"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
