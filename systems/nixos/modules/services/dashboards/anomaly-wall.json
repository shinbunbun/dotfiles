      "targets": [
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "${datasource}"
          },
          "format": 0,
          "rawSql": "SELECT \n  toStartOfMinute(detected_at) as time,\n  count() as anomaly_count\nFROM logs.anomalies\nWHERE detected_at >= now() - INTERVAL 24 HOUR\nGROUP BY time\nORDER BY time",
          "refId": "A"
        }
      ],
      "title": "異常検知数の推移（24時間）",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
