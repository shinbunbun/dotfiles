          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "${datasource}"
          },
          "format": 1,
          "rawSql": "SELECT count() as total_anomalies\nFROM logs.anomalies\nWHERE detected_at >= now() - INTERVAL 1 HOUR",
          "refId": "A"
        }
      ],
      "title": "過去1時間の異常検知数",
      "type": "gauge"
    },
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
